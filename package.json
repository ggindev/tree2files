{
  "name": "tree2files",
  "displayName": "Tree to File System",
  "description": "turm ascii trees into filesystems",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tree2files.createFoldersAndFiles",
        "title": "Create Folders and Files from Tree Structure"
      }
    ],
    "keybindings": [
      {
        "command": "tree2files.createFoldersAndFiles",
        "key": "ctrl+alt+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ggindev/tree3files.git"
 },
  "devDependencies": {
    
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
